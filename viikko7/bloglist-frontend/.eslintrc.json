{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
			"react/jsx-uses-react": [2],
			"react/jsx-uses-vars": [2],
			"eqeqeq": "error",
			"indent": [
				"warn",
				2
			],
			"no-console": 0
    },
    "plugins": [
        "react"
    ]
}
